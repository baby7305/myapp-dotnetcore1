{
  "Serilog": {
    "MinimumLevel": "Debug",
    "SyslogUrl": "localhost",
    "SyslogPort": "6514",
    "SyslogAppName": "myapp",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff} {Level:u4} {ProcessId} --- [{ThreadId,3}] {LoggerName,-40} : {Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  },
  "ConnectionStrings": {
    "AppDbContext": "Server=192.168.0.174;Port=3306;Database=myapp-dotnet1;User=root;Password=123456;"
  }
}
